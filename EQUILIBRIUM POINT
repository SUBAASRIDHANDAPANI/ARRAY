# include<stdio.h>
int main(){
    int sum=0,comp=0;
    int arr[]={1,3,5,2,2};
    for(int i=0;i<5;i++){
        sum+=arr[i];
    }
    for(int i=0;i<5;i++){
        sum=sum-arr[i];
        if(comp==sum){
            printf("%d",arr[i]);
            break;
        }
        comp=comp+arr[i];
    }
}
