# include<stdio.h>
int main(){
    int arr1[]={1,2,3,4,5};
    int arr2[]={1,3,4,5,6};
    int arr3[]={4,7,6,7,8};
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            for(int k=0;k<5;k++){
                if(arr1[i]==arr2[j] && arr2[j]==arr3[k] && arr1[i]==arr3[k]){
                    printf("%d",arr3[k]);
                }
            }
        }
    }
}
